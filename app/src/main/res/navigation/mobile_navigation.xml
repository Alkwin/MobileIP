<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.cringe.mobileip.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_orders_manager" />

    <fragment
        android:id="@+id/navigation_needier"
        android:name="com.cringe.mobileip.ui.orders.needier.NeedierFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_needier" />

    <fragment
        android:id="@+id/navigation_helper"
        android:name="com.cringe.mobileip.ui.orders.helper.HelperFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_helper" />

    <fragment
        android:id="@+id/navigation_orders"
        android:name="com.cringe.mobileip.ui.orders.OrdersManagerFragment"
        android:label="@string/title_orders"
        tools:layout="@layout/fragment_orders" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.cringe.mobileip.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.cringe.mobileip.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />


</navigation>